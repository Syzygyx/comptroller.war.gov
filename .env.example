# LLM API Keys (choose one or both)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LLM Configuration
LLM_PROVIDER=openai  # or anthropic
LLM_MODEL=gpt-4-turbo-preview  # or claude-3-opus-20240229

# OCR Configuration
TESSERACT_PATH=/opt/homebrew/bin/tesseract  # Auto-detected on macOS
OCR_ENGINE=tesseract  # or easyocr or both

# Download Configuration
MAX_CONCURRENT_DOWNLOADS=3
DOWNLOAD_DELAY_SECONDS=2

# Processing Configuration
MAX_FILE_SIZE_MB=50
BATCH_SIZE=10

# GitHub Configuration (for automated workflow)
GITHUB_TOKEN=your_github_token_here
GITHUB_REPO=yourusername/comptroller.war.gov
